# ============================================
# Parking App - Environment Variables
# ============================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control
# ============================================

# ============================================
# Frontend Environment Variables (Vite)
# ============================================
# These variables are prefixed with VITE_ to be accessible in the frontend

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/
# REQUIRED for map functionality
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Weather API Key (optional)
# Used for weather information in the app
VITE_WEATHER_API_KEY=your_weather_api_key_here

# API Key for backend API authentication
# Used for secure API communication
VITE_API_KEY=your_api_key_here

# Admin Credentials
# Used for admin panel authentication
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=your_secure_admin_password_here

# TON Wallet Address (optional)
# TON recipient address for payments
# Default: EQBahXxgN8ErwSBkCGEyuPEzg3-PdeodtGTbpSzGNjKs6OgV
VITE_TON_RECIPIENT_ADDRESS=YOUR_TON_WALLET_ADDRESS_HERE

# ============================================
# Backend / Telegram Bot Environment Variables
# ============================================

# Telegram Bot Token
# Get your bot token from: https://t.me/BotFather
# REQUIRED for Telegram bot functionality
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE

# Telegram Webhook URL (optional)
# Default: https://parkiraj.info/telegram-bot/webhook.php
TELEGRAM_WEBHOOK_URL=https://your-domain.com/telegram-bot/webhook.php

# Weather API Key (optional)
# Used for weather information in Telegram bot
WEATHER_API_KEY=your_weather_api_key_here

# TON Wallet Address (optional)
# TON recipient address for payments
# Default: EQBahXxgN8ErwSBkCGEyuPEzg3-PdeodtGTbpSzGNjKs6OgV
TON_RECIPIENT_ADDRESS=YOUR_TON_WALLET_ADDRESS_HERE

# Wallet Pay API Key (optional)
# Get your API key from: https://pay.wallet.tg/
# Used for automatic payments in Telegram bot
WALLET_PAY_API_KEY=your_wallet_pay_api_key_here

# ============================================
# API Authentication Keys
# ============================================

# General API Key
# Used for API authentication
API_KEY=your_api_key_here

# Web App API Key
# Used for web app API authentication
WEB_APP_API_KEY=YOUR_API_KEY_HERE

# Bot API Key (optional)
# Used for bot API authentication
# Falls back to WEB_APP_API_KEY if not set
BOT_API_KEY=your_bot_api_key_here
